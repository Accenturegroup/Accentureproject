<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.Accenture.Model.AssessmentPojo" table="assessment">
       <id column="assessID" name="assessID" type="int">
      <generator class="native"/>
    </id>
    <property name="assessName">
      <column name="assessName"/>
    </property>
      <property name="assessDate">
      <column name="assessDate"/>
    </property>
      <property name="date">
      <column name="date"/>
    </property>
  </class>
</hibernate-mapping>
