<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.Accenture.Model.learnerspojo" table="learner">
    <id column="learnerID" name="LID" type="int">
      <generator class="native"/>
    </id>
    <property name="LName">
      <column name="name"/>
    </property>
    <property name="LSurname">
      <column name="surname"/>
    </property>
    <property name="Title">
      <column name="title"/>
    </property>
    <property name="IDNumber">
      <column name="idNumber"/>
    </property>
    <property name="ContactNumber">
      <column name="contactNumber"/>
    </property>
    <property name="Email">
      <column name="email"/>
    </property>
    <property name="Gender">
      <column name="gender"/>
    </property>
    <property name="Race">
      <column name="race"/>
    </property>
    <property name="Group">
      <column name="group"/>
    </property>
    <property name="Password">
      <column name="password"/>
    </property>
    <property name="Status">
      <column name="status"/>
    </property>
  </class>
</hibernate-mapping>
