<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.Accenture.Model.LearnerAssessmentPojo" table="lassessment">
        <id column="ID" name="ID" type="int">
            <generator class="native"></generator>
        </id>
        <property name="AssessmentID">
            <column name="assessID"></column>
        </property>
        <property name="LearnerID">
            <column name="learnerID"></column>
        </property>
        <property name="Marks">
            <column name="mark"></column>
        </property>
      <property name="DateEntered">
            <column name="dateEntered"></column>
        </property>
    </class>
</hibernate-mapping>
